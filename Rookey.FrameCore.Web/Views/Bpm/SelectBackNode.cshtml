@using Rookey.Frame.Common;
@using Rookey.Frame.UIOperate;
@{ UIFrameFactory frameFactory = UIFrameFactory.GetInstance(this.Context.Request);
   Guid todoId = this.Context.Request.Query["todoId"].ObjToGuid();}
